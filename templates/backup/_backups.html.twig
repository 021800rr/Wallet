<table class="table table-hover table-sm">
    <thead>
    <tr>
        <th>{{ 'Date'|trans }}</th>
        <th class="text-end">{{ 'Amount'|trans }}</th>
    </tr>
    </thead>
    <tbody>
    {% for backup in backups %}
        <tr>
            <td>{{ backup.yearMonth }}</td>
            <td class="text-end row_money" id="backup_sum_of_amount{{ loop.index }}">{{ backup.sum_of_amount }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
