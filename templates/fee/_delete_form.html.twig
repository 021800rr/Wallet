<form method="post"
      id="fee_delete{{ loop.index }}"
      action="{{ path('fee_delete', {'id': fee.id}) }}"
      onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ fee.id) }}">
    <input type="image" src="{{ asset('images/delete.png') }}" alt="delete" width="14" height="14"/>
</form>
